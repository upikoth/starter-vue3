{
  "name": "starter-vue3",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "android:update": "vite build && cap sync android",
    "android:open": "cap open android",
    "ios:update": "vite build && cap sync ios",
    "ios:open": "cap open ios",
    "test:environment": "vitest run src/__tests__/environment.spec.ts",
    "test:unit": "vitest run --exclude 'src/__tests__/environment.spec.ts'",
    "test:e2e": "start-server-and-test 'NODE_ENV=test vite preview --port 4173' http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'NODE_ENV=test vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "lint:js": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:ts": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint:css": "stylelint \"src/**/*.{vue,scss}\"",
    "lint": "run-p lint:js lint:ts lint:css",
    "test": "run-p test:unit test:e2e",
    "format": "prettier --write src/",
    "update-types": "openapi-generator-cli generate -i https://raw.githubusercontent.com/upikoth/starter-go/main/docs/starter/docs.yml -g typescript-axios -o src/generated/starter"
  },
  "dependencies": {
    "@capacitor/android": "^4.7.3",
    "@capacitor/core": "^4.7.3",
    "@capacitor/ios": "^4.7.3",
    "@quasar/extras": "^1.16.11",
    "@sentry/vue": "^7.73.0",
    "pinia": "^2.0.32",
    "quasar": "^2.16.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@capacitor/cli": "^4.7.3",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@quasar/vite-plugin": "^1.7.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.11.24",
    "@upikoth/eslint-config-vue3": "^1.3.1",
    "@upikoth/stylelint-config-vue3": "^1.0.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.6.6",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^5.0.2",
    "prettier": "^2.8.8",
    "sass": "^1.32.12",
    "start-server-and-test": "^2.0.3",
    "typescript": "^5.4.5",
    "vite": "^4.1.4",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.19",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "run-p lint test"
  }
}
